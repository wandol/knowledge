/* Carousel
------------------------------------*/
.carousel { margin-bottom: 15px;}
.carousel-indicators { width: 100%; margin-left: 0; left: 0; bottom: -15px;}
.carousel-indicators li { display: inline-block; width: 19px; height: 2px; background: #c5c6c7; border: none; margin: 0;}
.carousel-indicators .active { width: 19px; height: 2px; background: #6894bf; border: none;}

.carousel-control { width: 10%;}
.carousel-control.left,
.carousel-control.right { background: none;}
.glyphicon-chevron-left:before { content: url(../img/ico-left.png);}
.glyphicon-chevron-right:before { content: url(../img/ico-right.png);}

/* p-info     modified 2018.09.20 
------------------------------------*/
.p-info .sub-info { color: #817466;}
.p-info .tag { display: inline-block; /* margin-top: 3px; margin-bottom: -10px; */}
.p-info .tag .btn { display: block; float: left; padding: 8px 10px; border: 2px solid #b7cee0; border-radius: 19px; background: #fff; min-width: 60px; text-align: center; text-decoration: none; color: #333; -webkit-box-shadow: none; box-shadow: none; outline: none; margin-right: 5px; /* margin-bottom: 5px; */}
.p-info .tag .btn:last-child { margin-right: 0;}
.p-info .tag .btn:hover { background: #e9f0f4;}
.p-info .tag .btn img { margin: -3px 5px 0 5px;} /* 2018.10.12 added */

.bubble>div>p, .bubble>div>img, .bubble>div>a, .bubble>div>ul, .bubble>div>button { margin: 5px 0px 5px 0px!important; }   
.bubble>div>p { white-space: pre-wrap; word-break: break-all;}

/*-----------------------------------------
    Air Korea
-------------------------------------------*/
.dust { width: 250px;}
/*.dust .title { margin-bottom: 10px;}*/
.dust .title div { float: left;}
.dust .title .item { font-size: 12px; margin-left: 40px; padding-top: 10px;}
.dust .title .item .time2 { font-size: 11px;}
.dust .title .count { font-size: 36px; font-weight: 700; margin-left: 10px;}
.dust .title .count .unit { font-size: 18px; font-weight: 400;}
.dust .title .count .unit sup { font-size: 12px;}

.dust .progress { background: none; -webkit-box-shadow: none; -ms-box-shadow: none; box-shadow: none; height: 40px; margin-bottom: 0; width:250px;}
.dust .progress .progress-bar { position: relative; background: none; -webkit-box-shadow: none; -ms-box-shadow: none; box-shadow: none;}
.dust .progress .progress-bar .tooltip { opacity: 1; position: absolute; right: -20px; line-height: 24px;}
.dust .progress.good .tooltip-inner { background: #32a1ff;}
.dust .progress.soso .tooltip-inner { background: #00c73c;}
.dust .progress.bad .tooltip-inner { background: #fd9b5a;}
.dust .progress.vbad .tooltip-inner { background: #ff5959;}

.dust .progress.good .tooltip-arrow { border-top-color: #32a1ff;}
.dust .progress.soso .tooltip-arrow { border-top-color: #00c73c;}
.dust .progress.bad .tooltip-arrow { border-top-color: #fd9b5a;}
.dust .progress.vbad .tooltip-arrow { border-top-color: #ff5959;}

.dust .range-box { margin-bottom: 10px;}
.dust .range-box .bar li { float: left; text-align: right; width: 61px; display: inline-block; font-size: 10px;  border-top: 2px solid #ccc; padding-top: 3px;}
.dust .range-box .bar li+li { margin-left: 2px;}
.dust .range-box .bar li.good { border-top-color: #32a1ff;}
.dust .range-box .bar li.soso { border-top-color: #00c73c;}
.dust .range-box .bar li.bad { border-top-color: #fd9b5a;}
.dust .range-box .bar li.vbad { border-top-color: #ff5959;}

.dust .list-air { width: 250px;}
.dust > .list-air > ul > li { display: inline-block; float: left; width: 125px;}
.dust .list-air ul li { font-size: 11px;}
.dust .list-air ul > li > ul > li  { line-height: 20px;}
.dust .list-air ul li ul li span { width: 65px; display: inline-block; padding-left: 10px;}
.dust .list-air strong.good { color: #32a1ff;}
.dust .list-air strong.soso {color: #00c73c;}
.dust .list-air strong.bad { color: #fd9b5a;}
.dust .list-air strong.vbad { color: #ff5959;}