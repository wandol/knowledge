/* common class*/
.clearfix:after,
.clearfix:before { content: '.'; display: block; width: 0; height: 0; text-indent: -9999px; overflow: hidden; clear: both;}
.hide { display: none;}
.screenout { display: block; width: 0; height: 0; text-indent: -9999px; overflow: hidden;}

.chat-dialog { position: absolute; right: 0; }
::-webkit-scrollbar { width: 5px; border: 0px; }
::-webkit-scrollbar-track { background: #f3f4f5; }
::-webkit-scrollbar-thumb { background: #f3f4f5; }
::-webkit-scrollbar-thumb:hover { background: #bebebe; }
#chat-message .modal-title { cursor: pointer; text-decoration: none; color: #fff; }
#chat-message .modal-body { visibility: hidden; }
.message { list-style: none; }
.message li { -webkit-transition: all .5s; transition: all .5s; position: relative; display: block; }

.message li.bot p { white-space: pre-wrap; word-break: break-all; }
.message li.bot p.title { font-size: 16px; font-weight: 900; }
.message li.bot p.subtitle { font-size: 12px; }
.message li.bot img { max-width: 100%; height: auto;}
 
.bubble>div>p, .bubble>div>img, .bubble>div>a, .bubble>div>ul, .bubble>div>button { margin: 5px 0px 5px 0px!important; }   
.navbar-nav.navbar-user li a.setup{margin-left: -5px; font-size: 16px;}

.panel-wrap .panel .graph-body {padding: 5px;}
.graph-body div.legend { float: right; width: 10%; margin-top: 5px; line-height: 1.5em;}
.legend .legendLabel {padding-left: 5px!important; font-size: x-small;}

.ellipsis {white-space: nowrap; overflow: hidden; text-overflow: ellipsis; -o-text-overflow: ellipsis; }
.chart svg text {cursor: default!important;}
