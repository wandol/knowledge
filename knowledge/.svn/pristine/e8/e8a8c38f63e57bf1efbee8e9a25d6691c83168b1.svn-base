/*-----------------------------------------
    Air Korea
-------------------------------------------*/
.bubble .dust { width: 250px;}
/*.bubble .dust .title { margin-bottom: 10px;}*/
.bubble .dust .title div { float: left;}
.bubble .dust .title .item { font-size: 12px; margin-left: 40px; padding-top: 10px;}
.bubble .dust .title .item .time2 { font-size: 11px;}
.bubble .dust .title .count { font-size: 36px; font-weight: 700; margin-left: 10px;}
.bubble .dust .title .count .unit { font-size: 18px; font-weight: 400;}
.bubble .dust .title .count .unit sup { font-size: 12px;}

.bubble .dust .progress { background: none; -webkit-box-shadow: none; -ms-box-shadow: none; box-shadow: none; height: 40px; margin-bottom: 0; width:250px;}
.bubble .dust .progress .progress-bar { position: relative; background: none; -webkit-box-shadow: none; -ms-box-shadow: none; box-shadow: none;}
.bubble .dust .progress .progress-bar .tooltip { opacity: 1; position: absolute; right: -20px; line-height: 24px;}
.bubble .dust .progress.good .tooltip-inner { background: #32a1ff;}
.bubble .dust .progress.soso .tooltip-inner { background: #00c73c;}
.bubble .dust .progress.bad .tooltip-inner { background: #fd9b5a;}
.bubble .dust .progress.vbad .tooltip-inner { background: #ff5959;}

.bubble .dust .progress.good .tooltip-arrow { border-top-color: #32a1ff;}
.bubble .dust .progress.soso .tooltip-arrow { border-top-color: #00c73c;}
.bubble .dust .progress.bad .tooltip-arrow { border-top-color: #fd9b5a;}
.bubble .dust .progress.vbad .tooltip-arrow { border-top-color: #ff5959;}

.bubble .dust .range-box { margin-bottom: 10px;}
.bubble .dust .range-box .bar li { float: left; text-align: right; width: 61px; display: inline-block; font-size: 10px;  border-top: 2px solid #ccc; padding-top: 3px;}
.bubble .dust .range-box .bar li+li { margin-left: 2px;}
.bubble .dust .range-box .bar li.good { border-top-color: #32a1ff;}
.bubble .dust .range-box .bar li.soso { border-top-color: #00c73c;}
.bubble .dust .range-box .bar li.bad { border-top-color: #fd9b5a;}
.bubble .dust .range-box .bar li.vbad { border-top-color: #ff5959;}

.bubble .dust .list-air { width: 250px;}
.bubble .dust > .list-air > ul > li { display: inline-block; float: left; width: 125px;}
.bubble .dust .list-air ul li { font-size: 11px;}
.bubble .dust .list-air ul > li > ul > li  { line-height: 20px;}
.bubble .dust .list-air ul li ul li span { width: 65px; display: inline-block; padding-left: 10px;}
.bubble .dust .list-air strong.good { color: #32a1ff;}
.bubble .dust .list-air strong.soso {color: #00c73c;}
.bubble .dust .list-air strong.bad { color: #fd9b5a;}
.bubble .dust .list-air strong.vbad { color: #ff5959;}

.bubble>div>p, .bubble>div>img, .bubble>div>a, .bubble>div>ul, .bubble>div>button { margin: 5px 0px 5px 0px!important; }   
.bubble iframe {width: 100%;}

