/* trafic
-------------------------------------------*/
.bus-wrap { font-size: 16px; font-weight: 500; color: #181818; border-top: 1px solid #e8f0f6; padding-top: 6px; margin-top: 14px; background-color:white; padding:10px;}
.bus-wrap:first-child { border: 0; padding-top: 0; margin-top: 0;}

.bus-wrap .bus-time {margin-bottom: 12px; border-bottom:1px solid #00000059; padding:6px;}
/* .bus-wrap .bus-time a { float: right; width: 18px; height: 11px; margin-top: 6px; background: url(../img/ico-updown.png) no-repeat 0 0;} */
.bus-wrap .bus::after,
.bus-wrap .bus::before { content: ""; clear: both; display: block;}
.bus-wrap .bus span { display: block; line-height: 28px; float: left;}
.bus-wrap .bus .number { padding-left: 20px; width:65px;}
.bus-wrap .bus .blue { background: url(../img/bus/bus_blue.png) no-repeat left;}
.bus-wrap .bus .skyblue { background: url(../img/bus/bus_skyblue.png) no-repeat left;}
.bus-wrap .bus .green { background: url(../img/bus/bus_green.png) no-repeat left;}
.bus-wrap .bus .ygreen { background: url(../img/bus/bus_ygreen.png) no-repeat left;}
.bus-wrap .bus .red { background: url(../img/bus/bus_red.png) no-repeat left;}
.bus-wrap .bus .grey { background: url(../img/bus/bus_grey.png) no-repeat left;}

.bus-wrap .bus .staition { font-size: 12px; font-weight: 400;}
.bus-wrap .line { width: 1px; height: 25px; background: #c2c2c2; margin-left: 7px;}
.bus-wrap .line:last-child{display:none;}
.bus-wrap.brief .bus .number::after,
.bus-wrap .bus .staition::after { content: url(../img/ico-arr-right.png); padding-left: 5px; padding-right: 5px;}
.bus-wrap.brief .bus .number:last-child::after,
.bus-wrap .bus .staition:last-child::after{ content: "";}
.bus-wrap.brief { }
.bus-wrap.brief .bus-time a { background-position: -18px 0;}
.bus-wrap.brief .bus .number { font-size: 14px; font-weight: 400; margin-right: 0;}
.bubble .bus-list  {border-bottom:1px solid #968282 !important; width:255px !important; background-color:white !important; padding:10px !important; cursor:pointer !important;}
.bubble .bus-list a {font-weight:bold; color:#1a6dc5e0;}
.bubble .bus-length {font-weight:bold; color:#968282; font-size:12px;}
.bubble .bus-list h6 {color:#968282;}

/* movie
-------------------------------------------*/
.m-rank { margin-top: 10px;}
.m-rank > li { border-top: 1px solid #e8f0f6; padding-bottom: 7px; padding-top: 7px;}
.m-rank > li:first-child { border: none; }
.m-rank > li:last-child { padding-bottom: 0;}
.m-rank > li > ul::after,
.m-rank > li > ul::before { content: ""; clear: both; display: block;}
.m-rank > li > ul >li { float: left;}
.m-rank .m-title { font-size: 16px; font-weight: 500; color: #181818; margin-bottom: 5px;}
.m-rank .m-title .up { background: url(../img/bullet_up.png) no-repeat 1px; padding-left: 12px; padding-top: 2px; margin-left: 3px; font-size: 10px; }
.m-rank .m-title .down { background: url(../img/bullet_down.png) no-repeat 1px; padding-left: 12px; padding-top: 2px; margin-left: 3px; font-size: 10px; }
.m-rank .m-title .bar { background: url(../img/bullet_bar.png) no-repeat 1px; padding-left: 12px; padding-top: 2px; margin-left: 3px; font-size: 10px; }
.m-rank .m-title li span { display: block; width: 13px;}
.m-rank .m-title .bar span { text-indent: -9999px;}
.m-rank .m-info { font-size: 13px; font-weight: 300; margin-left: 51px;}
.m-rank .m-info li:nth-child(odd) { color: #7b7b7b; margin-right: 5px;}
.m-rank .m-info li:nth-child(odd):last-child { margin-right: 0;}
.m-rank .m-info li:nth-child(even)::after { display: inline-block; content: "l"; color: #b9b9b9; padding-left: 5px; padding-right: 5px;}
.m-rank .m-info li:nth-child(even):last-child::after { content: none;}


