/* Carousel
------------------------------------*/
.carousel-inner .item {padding: 0 30px;} 
.carousel-indicators { width: 100%; margin-left: 0; left: 0; bottom: 0; position:relative; padding-top:30px;}
.carousel-indicators li { display: inline-block!important; width: 19px; height: 5px; background: #c5c6c7; border: none; margin: 0;}
.carousel-indicators .active { width: 19px; height: 5px; background: #6894bf; border: none;}
.carousel-control.left, .carousel-control.right { background: none; color: #1e63a7; text-shadow:none; width: 10%; z-index: 100;}
.carousel-control span {font-size:17px!important;}

