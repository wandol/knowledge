/* 콘텐츠 */
#container {
  position: relative;
  width: calc(100% - 291px);
  height: 100%;
  margin-left: 291px;
}
#contentWrap {
  background: #f8f8f9;
}
#contentWrap.after {
  content: '';
  display: block;
  width: 100%;
  clear: both;
}
#contentWrap .page-title #spot {
  padding: 20px 30px;
  position: relative;
}
#contentWrap .page-title #spot .page-info {
  color: #393939;
  font-size: 20px;
  font-weight: 500;
  margin-bottom: 5px;
  font-family: "Noto Sans KR";
}
#contentWrap .page-title #spot .btn-refresh {
  display: block;
  width: 32px;
  height: 32px;
  border-radius: 50%;
  position: absolute;
  top: 32px;
  right: 35px;
  background: #fff;
  z-index: 1;
}
#contentWrap .page-title #spot .btn-refresh i {
  display: block;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  font-size: 25px;
  -webkit-transition: all 0.2s linear;
  -o-transition: all 0.2s linear;
  transition: all 0.2s linear;
  font-size: 20px;
}
#contentWrap .page-title #spot .btn-refresh:hover i {
  transform: translate(-50%, -50%) rotate(180deg);
}
#contentWrap .page-content {
  position: relative;
  z-index: 1;
}
#contentWrap .page-content .inner-wrap {
  padding: 20px 30px 50px;
  position: relative;
}
