@charset "utf-8";
/* 허브팝 사용자 레이아웃 CSS */
/* A2M KSI 2020-04-16 */
.no-radius {
  border-radius: 0;
}
.clear {
  content: '';
  display: block;
  width: 100%;
  clear: both;
}
/* 공통 */
ul,
ol,
li,
dl,
dt,
dd {
  margin: 0;
  padding: 0;
  list-style: none;
}
​ p,
h1,
h2,
h3,
h4,
h5,
h6 {
  margin: 0;
  padding: 0;
}
​ a {
  color: inherit;
  text-decoration: none;
}
​ img {
  border: 0;
  vertical-align: middle;
  font-size: 0;
  max-width: 100%;
}
​ table {
  border-collapse: collapse;
  width: 100%;
}
/* 부트스트랩 스타일 재정의 */
a {
  color: #333;
}
a:hover,
a:focus,
a:visited {
  text-decoration: none;
}
/* 레이아웃 */
html {
  position: relative;
  width: 100%;
  min-width: 1440px;
  height: auto;
  min-height: 100vh;
}
.user-site {
  position: relative;
  width: 100%;
  min-height: inherit;
}
#pageWrap {
  position: relative;
  width: 100%;
  min-height: inherit;
  height: 100%;
  background: #f8f8f9;
}
#pageWrap::after {
  content: '';
  display: block;
  width: 100%;
  clear: both;
}
#pageWrap.dashboard-full #lnbWrap {
  display: none;
}
#pageWrap.page-blank #container {
  margin-left: 70px;
  width: calc(100% - 70px);
}
/* header */
#header {
  position: fixed;
  top: 0;
  left: 0;
  min-height: 880px;
  height: inherit;
  z-index: 20;
  width: 70px;
  background: linear-gradient(to bottom, #2a5298, #1e3c72);
}
#header::after {
  content: '';
  display: block;
  width: 100%;
  clear: both;
}
#header .left-wrap {
  position: relative;
  width: 70px;
  min-height: 710px;
  height: 100%;
  max-height: 100vh;
}
#header .left-wrap > .header-top h1 {
  width: 100%;
  height: 80px;
  margin: 0 auto;
}
#header .left-wrap > .header-top h1 > a {
  display: block;
  width: 100%;
  height: 100%;
  background: url('../../assets/img/user/logo_user.png') center no-repeat;
}
#header .left-wrap > .header-mid {
  border-top: 1px solid #1b3e7c;
}
/* quick & group */
#header .left-wrap > .header-mid .lnb-depth1-item > a {
  display: block;
  width: 100%;
  height: 60px;
  background-repeat: no-repeat;
  background-position: center;
  background-color: transparent;
  border: 0;
  -webkit-transition: all 0.2s linear;
  -o-transition: all 0.2s linear;
  transition: all 0.2s linear;
  padding: 21px 26px;
}
#header .left-wrap > .header-mid .lnb-depth1-item > a:hover {
  background-color: #22437e;
}
#header .left-wrap > .header-mid .lnb-depth1-item > a > span {
  display: inline-block;
  overflow:hidden;
  width:18px;
  height:18px;
}
#header .left-wrap > .header-mid .lnb-depth1-item > a > span > img {
  transform: translateY(-18px);
}
#header .left-wrap > .header-mid #gnb > .lnb-depth1-item.active {
  background-color: #193971;
}
#header .left-wrap > .header-mid .lnb-depth1-item.active > a > span > img {
  transform: translateY(0px);
}
/* group */
#header .left-wrap > .header-mid #gnb > .lnb-depth1-item.item1 {
  border-top: 1px solid #1b3e7c;
}
#header .left-wrap > .header-mid #gnb > .lnb-depth1-item.item4 .tooltip {
  width: 170px;
}
#header .left-wrap > .header-mid #gnb > .lnb-depth1-item .tooltip {
  display: block;
  left: 55px !important;
  min-width: 100px;
  max-width: 300px;
  z-index: 30;
}
#header .left-wrap > .header-mid #gnb > .lnb-depth1-item .tooltip.in {
  opacity: 1;
}
#header .left-wrap > .header-mid #gnb > .lnb-depth1-item .tooltip-inner {
  padding: 10px 14px;
  color: #454545;
  text-align: center;
  background-color: #fff;
  border-radius: 3px;
  font-size: 14px;
  display: block;
  min-width: 95px;
  box-shadow: 3px 3px 3px 1px rgba(0, 0, 0, 0.1);
  box-sizing: border-box;
}
#header .left-wrap > .header-mid #gnb > .lnb-depth1-item .tooltip.right .tooltip-arrow {
  border-right-color: #fff;
}
#header .left-wrap > .header-bottom {
  border-top: 1px solid #1b3e7c;
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 140px;
}
#header .left-wrap > .header-bottom .quick-list > li {
  position: relative;
}
#header .left-wrap > .header-bottom .quick-list > li > a {
  display: block;
  color: #fff;
  height: 60px;
  line-height: 60px;
  font-size: 19px;
  text-align: center;
  position: relative;
}
#header .left-wrap > .header-bottom .quick-list > li > a .new {
  display: block;
  width: 6px;
  height: 6px;
  border: 1px solid #fff;
  background: #f10f4f;
  border-radius: 50%;
  position: absolute;
  top: 24px;
  left: 36px;
}
#header .left-wrap > .header-bottom .quick-list > li.notification-area.active .notification-wrap {
  opacity: 1;
  visibility: visible;
  z-index: 2;
}
#header .left-wrap > .header-bottom .quick-list > li.user-info-area.active .user-info-wrap {
  opacity: 1;
  visibility: visible;
  z-index: 2;
}
#header .left-wrap > .header-bottom .notification-wrap {
  position: absolute;
  bottom: -30px;
  left: 84px;
  background: #fff;
  border-radius: 3px;
  box-shadow: 0 0 8px rgba(0, 0, 0, 0.1);
  width: 360px;
  overflow: hidden;
  opacity: 0;
  visibility: hidden;
  z-index: -1;
  -webkit-transition: all 0.2s linear;
  -o-transition: all 0.2s linear;
  transition: all 0.2s linear;
}
#header .left-wrap > .header-bottom .notification-wrap .notification-title {
  color: #fff;
  background: #545c6b;
  text-align: center;
  padding: 14px;
  font-weight: 600;
  font-size: 13px;  
}
#header .left-wrap > .header-bottom .notification-wrap .notification-content > li {
  border-bottom: 1px solid #eaeaea;
}
#header .left-wrap > .header-bottom .notification-wrap .notification-content > li > a {
  display: block;
  padding: 14px 15px;
}
#header .left-wrap > .header-bottom .notification-wrap .notification-content > li .round {
  display: inline-block;
  width: 30px;
  height: 30px;
  border-radius: 50%;
  color: #fff;
  text-align: center;
  background: #4cd0e1;
  font-size: 12px;
  line-height: 28px;
  vertical-align: top;
  margin-right: 15px;
  margin-top: 5px;
}
#header .left-wrap > .header-bottom .notification-wrap .notification-content > li .text {
  display: inline-block;
  vertical-align: top;
  width: calc(100% - 50px);
}
#header .left-wrap > .header-bottom .notification-wrap .notification-content > li .text .title {
  display: block;
  font-weight: 400;
  color: #414141;
  margin-bottom: 5px;
  font-size: 13px;
}
#header .left-wrap > .header-bottom .notification-wrap .notification-content > li .text .date {
  display: block;
  color: #7f7f7f;
  font-size: 12px;
  float: right;
}
#header .left-wrap > .header-bottom .notification-wrap .notification-content > li.notification-type2 .round {
  background: #8fa4ae;
}
#header .left-wrap > .header-bottom .notification-wrap .notification-content > li.notification-type3 .round {
  background: #63b5f7;
}
#header .left-wrap > .header-bottom .notification-wrap .notification-content > li.notification-type4 .round {
  background: #7885cb;
}
#header .left-wrap > .header-bottom .notification-wrap .notification-show a {
  display: block;
  text-align: center;
  color: #767676;
  font-size: 13px;
  padding: 10px;
}
#header .left-wrap > .header-bottom .user-info-wrap {
  position: absolute;
  bottom: 10px;
  left: 84px;
  background: #fff;
  border-radius: 3px;
  box-shadow: 0 0 8px rgba(0, 0, 0, 0.1);
  width: 210px;
  overflow: hidden;
  opacity: 0;
  visibility: hidden;
  z-index: -1;
  -webkit-transition: all 0.2s linear;
  -o-transition: all 0.2s linear;
  transition: all 0.2s linear;
}
#header .left-wrap > .header-bottom .user-info-wrap li:nth-child(2) {
  padding-top:7px;
}
#header .left-wrap > .header-bottom .user-info-wrap li:last-child {
  margin-top:7px;
}
#header .left-wrap > .header-bottom .user-info-wrap .user-info {
  padding: 15px;
  border-bottom: 1px solid #eee;
}
#header .left-wrap > .header-bottom .user-info-wrap .user-info::after {
  content: '';
  display: block;
  width: 100%;
  clear: both;
}
#header .left-wrap > .header-bottom .user-info-wrap .user-info .user-img {
  width: 35px;
  height: 35px;
  border-radius: 50%;
  overflow: hidden;
  float: left;
  background: #63b5f7;
  text-align: center;
  color: #fff;
  font-size: 15px;
  line-height: 35px;
}
#header .left-wrap > .header-bottom .user-info-wrap .user-info .user-img i {
  font-size: 18px;
  line-height: 35px;
}
#header .left-wrap > .header-bottom .user-info-wrap .user-info .user-name {
  float: left;
  width: calc(100% - 40px);
  padding-left: 10px;
  padding-top: 8px;
  display: block;
  text-overflow: ellipsis;
  white-space: nowrap;
  word-wrap: normal;
  overflow: hidden;
  font-size: 13px;
}
#header .left-wrap > .header-bottom .user-info-wrap > li > a {
  display: block;
  padding: 7px 20px;
  font-size: 13px;
  -webkit-transition: all 0.2s linear;
  -o-transition: all 0.2s linear;
  transition: all 0.2s linear;
}
#header .left-wrap > .header-bottom .user-info-wrap > li > a:hover {
  background: #f8f8f8;
}
#header .left-wrap > .header-bottom .user-info-wrap > li > a i {
  width: 21px;
}
#header .left-wrap > .header-bottom .user-info-wrap > li:first-child > a {
  padding-top: 22px;
}
#header .left-wrap > .header-bottom .user-info-wrap > li > a .feather {
  color: #868686;
  font-size:16px;
}
#header .left-wrap > .header-bottom .user-info-wrap > li.logout {
  border-top: 1px solid #eee;
}
#header .left-wrap > .header-bottom .user-info-wrap > li.logout > a {
  padding-top: 10px;
  padding-bottom: 15px;
}
#header .left-wrap > .header-bottom .user-info-wrap > li.logout i {
  color: #dd124b;
  font-size: 16px;
  vertical-align: middle;
}
#header .left-wrap > .header-bottom .user-info-wrap > li.logout span {
  font-size: 13px;
}

#pageWrap.all-menu-open {
  height: 100vh;
  overflow-y: hidden;
}
#pageWrap.all-menu-open #header {
  height: 100%;
  min-height: 1100px;
}
/* 전체 메뉴 */
#allMenu {
  position: absolute;
  top: 0;
  left: 70px;
  width: 0;
  height: inherit;
  background: #1a386d;
  color: #fff;
  z-index: -1;
  opacity: 0;
  visibility: hidden;
  overflow: hidden;
  display: none;
  z-index: 10;
}
#allMenu.active {
  opacity: 1;
  visibility: visible;
  display: block;
  z-index: 10;
  width: calc(100% - 70px);
  overflow-y: auto;
}
#allMenu .all-menu-wrap {
  width: 1202px;
  padding: 0 30px;
}
#allMenu .all-menu-top {
  width: 100%;
  height: 80px;
}
#allMenu .all-menu-top::after {
  content: '';
  display: block;
  width: 100%;
  clear: both;
}
#allMenu .all-menu-top .title {
  float: left;
  font-size: 20px;
  line-height: 80px;
  font-family: "Noto Sans KR";
}
#allMenu .all-menu-top .btn-close {
  display: block;
  float: right;
  width: 25px;
  height: 25px;
  position: relative;
  margin-top: 25px;
}
#allMenu .all-menu-top .btn-close span {
  display: block;
  position: absolute;
  background: #fff;
  margin-top: 25px;
}
#allMenu .all-menu-top .btn-close span.bar1 {
  width: 2px;
  height: 25px;
  top: -25px;
  left: calc(50% - 1px);
  transform: rotate(45deg);
}
#allMenu .all-menu-top .btn-close span.bar2 {
  width: 25px;
  height: 2px;
  top: -13px;
  left: 0;
  transform: rotate(45deg);
}
#allMenu .category-list::after {
  content: '';
  display: block;
  width: 100%;
  clear: both;
}
#allMenu .category-list > li {
  display: inline-block;
  vertical-align: top;
  width: 33%;
  padding-right: 40px;
  margin-bottom: 40px;
}
#allMenu .category-list .inner .title {
  color: #9ab9f1;
  margin-bottom: 10px;
  font-weight: 600;
}
#allMenu .category-list .all-lnb-depth2 .all-depth2-item .depth2 {
  padding: 10px;
  border-bottom: 1px solid #132a53;
  position: relative;
}
#allMenu .category-list .all-lnb-depth2 .all-depth2-item .depth2 .link {
  position: relative;
}
#allMenu .category-list .all-lnb-depth2 .all-depth2-item .depth2 .link:hover {
  text-decoration: none;
}
#allMenu .category-list .all-lnb-depth2 .all-depth2-item .depth2 .link .depth-icon-img {
  display: inline-block;
  vertical-align: top;
  margin-right: 8px;
  margin-top: 4px;
  width: 16px;
  height: 16px;
  overflow: hidden;
}
#allMenu .category-list .all-lnb-depth2 .all-depth2-item .depth2 .link .depth-icon-img img {
  vertical-align: top;
}
#allMenu .category-list .all-lnb-depth2 .all-depth2-item .depth2 .link span {
  display: inline-block;
  vertical-align: top;
  color: #fff;
  font-weight: 600;
  max-width: 260px;
  font-size:14px;
}
#allMenu .category-list .all-lnb-depth2 .all-depth2-item .depth2 .bookmark {
  display: block;
  position: absolute;
  top: 15px;
  right: 10px;
  width: 16px;
  height: 16px;
  overflow: hidden;
  line-height: 16px;
}
#allMenu .category-list .all-lnb-depth2 .all-depth2-item .depth2 .bookmark::before {
  content: '\ea1d';
  font-family: 'xeicon';
  font-size: 15px;
  line-height: 15px;
  color: #97a4bd;
}
#allMenu .category-list .all-lnb-depth2 .all-depth2-item .depth2 .bookmark:hover::before {
  content: '\ea1c';
}
#allMenu .category-list .all-lnb-depth2 .all-depth2-item .depth2 .bookmark.active::before {
  content: '\ea1c';
}
#allMenu .category-list .all-lnb-depth3 {
  padding: 13px;
}
#allMenu .category-list .all-lnb-depth3 .all-depth3-item {
  margin-bottom: 10px;
}
#allMenu .category-list .all-lnb-depth3 .all-depth3-item:last-child {
  margin-bottom: 0;
}
#allMenu .category-list .all-lnb-depth3 .all-depth3-item > a {
  display: block;
  font-size: 13px;
  color: #cad8ef;
}
#allMenu .category-list .all-lnb-depth3 .all-depth3-item > a .depth-icon-img {
  display: inline-block;
  vertical-align: top;
  margin-right: 5px;
  margin-top: 2px;
  width: 16px;
  height: 16px;
  overflow: hidden;
}
#allMenu .category-list .all-lnb-depth3 .all-depth3-item > a .depth-icon-img img {
  vertical-align: top;
  transform: translateY(-16px);
}
#allMenu .category-list .all-lnb-depth3 .all-depth3-item > a > span {
  display: inline-block;
  vertical-align: top;
  max-width: 270px;
}
#allMenu .category-list .all-lnb-depth3 .all-depth3-item > a:hover {
  text-decoration: none;
  color: #fff;
}
#allMenu .category-list .all-lnb-depth3 .all-depth3-item > a:hover .depth-icon-img img {
  transform: translateY(0);
}
#allMenu .category-list .all-lnb-depth3 .all-depth3-item.on a {
  color: #fff;
}
#allMenu .category-list .all-lnb-depth3 .all-depth3-item.on a .depth-icon-img img {
  transform: translateY(0px);
}
/* 오른쪽 서브메뉴 */
#lnbWrap {
  position: fixed;
  top: 0;
  left: 70px;
  width: 221px;
  height: 100%;
  background-color: #fff;
  min-height: inherit;
  border-right: 1px solid #e5e6ed;
  z-index: 2;
}
#lnbWrap #lnb .title-project {
  height: 81px;
  position: relative;
  padding: 20px 35px 20px 20px;
  border-bottom: 1px solid #e5e6ed;
}
#lnbWrap #lnb .title-project .text {
  color: #434343;
  font-size:13px;
  font-weight: 600;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical; 
}
#lnbWrap #lnb .title-project .dropdown {
  position: absolute;
  top: 20px;
  right: 3px;
}
#lnbWrap #lnb .title-project .dropdown .feather-icon {
  color: #a6b4cd;
  font-size: 20px;
  line-height: 20px;
}
#lnbWrap #lnb .title-project .dropdown .dropdown-menu {
  top: 0;
  left: 100%;
  width: 180px;
}
#lnbWrap #lnb .title-project .dropdown .dropdown-menu li a {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
#lnbWrap #lnb .title-project .dropdown .dropdown-menu li a .feather {
  font-size: 14px;
  color: #7d838e;
}
#lnbWrap #lnb .btn-dropdown-depth {
  display: block;
  position: absolute;
  top: 0px;
  right: 0;
  width: 22px;
  height: 22px;
  z-index: 2;
}
#lnbWrap #lnb .btn-dropdown-depth::before {
  content: '';
  width: 12px;
  height: 2px;
  background: #939393;
  position: absolute;
  top: 9px;
  right: 0;
}
#lnbWrap #lnb .btn-dropdown-depth:after {
  content: '';
  width: 2px;
  height: 12px;
  background: #939393;
  position: absolute;
  top: 4px;
  right: 5px;
}
#lnbWrap #lnb .scroll-bar {
  width: 100%;
  height: calc(100vh - 90px);
  position: relative;
}
#lnbWrap #lnb .scroll-bar .mCSB_inside > .mCSB_container {
  margin-right: 0;
}
#lnbWrap #lnb .scroll-bar .mCS-minimal.mCSB_scrollTools .mCSB_dragger .mCSB_dragger_bar {
  background-color: rgba(221, 221, 221, 0.6);
}
#lnbWrap #myService {
  padding: 15px 20px;  
}
#lnbWrap #myService .sub-title {
  position: relative;
}
#lnbWrap #myService .sub-title span {
  color: #668ba8;
  font-weight: 600;
  font-size: 13px;
}
#lnbWrap #myService .sub-title .btn-dropdown {
  display: block;
  position: absolute;
  top: 8px;
  right: 0;
  width: 14px;
  height: 8px;
  background-image: url('../../assets/img/user/lnb_icon_dropdown.png');
  background-position: 0 bottom;
  background-repeat: no-repeat;
  -webkit-transition: all 0.2s linear;
  -o-transition: all 0.2s linear;
  transition: all 0.2s linear;
}
#lnbWrap #myService .lnb-depth2 {
  display: none;
}
#lnbWrap #myService .lnb-depth2 > li {
  margin-bottom: 15px;
}
#lnbWrap #myService .lnb-depth2 > li .depth2 {
  margin-bottom: 15px;
  position: relative;
}
#lnbWrap #myService .lnb-depth2 > li a.link {
  display: block;
  color: #303030;
  font-weight: 600;
  position: relative;
  width: calc(100% - 15px);
  z-index: 1;
}
#lnbWrap #myService .lnb-depth2 > li a.link .depth-icon-img {
  display: inline-block;
  width: 16px;
  height: 16px;
  overflow: hidden;
  vertical-align: top;
  margin-right: 5px;
  margin-top: 2px;
}
#lnbWrap #myService .lnb-depth2 > li a.link .depth-icon-img img {
  vertical-align: top;
  transform: translateY(-32px);
}
#lnbWrap #myService .lnb-depth2 > li a.link span {
  display: inline-block;
  vertical-align: top;
  max-width: 140px;
  font-size:13px;
}
#lnbWrap #myService .lnb-depth2 > li a.bookmark {
  display: none;
}
#lnbWrap #myService .lnb-depth2 > li.active .btn-dropdown-depth:after {
  display: none;
}
#lnbWrap #myService .lnb-depth2 > li.active .all-lnb-depth3 {
  display: block;
}
#lnbWrap #myService .lnb-depth2 > li.on .link {
  color: #3f64a7;
}
#lnbWrap #myService .lnb-depth2 > li.on .link .depth-icon-img img {
  transform: translateY(-16px);
}
#lnbWrap #myService .all-lnb-depth3 {
  display: none;
  padding-left: 15px;
}
#lnbWrap #myService .all-lnb-depth3 > li {
  margin-bottom: 10px;
}
#lnbWrap #myService .all-lnb-depth3 > li > a {
  display: block;
  font-size: 13px;
  color: #373737;
}
#lnbWrap #myService .all-lnb-depth3 > li > a .depth-icon-img {
  display: none;
}
#lnbWrap #myService .all-lnb-depth3 > li > a span {
  display: inline-block;
  position: relative;
  padding-left: 10px;
}
#lnbWrap #myService .all-lnb-depth3 > li > a span::before {
  content: '';
  width: 3px;
  height: 3px;
  background: #5e7aad;
  position: absolute;
